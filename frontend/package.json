{
  "name": "simple-s3-storage-frontend",
  "version": "1.0.0",
  "description": "Frontend for Simple S3-like Storage System",
  "private": true,
  "dependencies": {
    "axios": "^1.10.0",
    "lucide-react": "^0.263.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "build:prod": "npm run build && npm run optimize",
    "optimize": "npx workbox-cli generateSW workbox-config.js",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,css,md}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "prefer-const": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "homepage": "http://.",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "s3",
    "storage",
    "file-upload",
    "react",
    "cloud-storage",
    "bucket",
    "file-sharing"
  ],
  "author": "Simple S3 Storage <admin@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/simple-s3-storage.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/simple-s3-storage/issues"
  }
}
